#include "rk3399.dtsi"

/ {
	model = "LX-R3S RK3399 Board";
	compatible = "rockchip,rk3399";

	aliases {
		ethernet0 = &gmac;
		mmc0 = &sdio0;
		mmc1 = &sdmmc;
		mmc2 = &sdhci;
	};

	chosen {
		stdout-path = "serial2:1500000n8";
	};
