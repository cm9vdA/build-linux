		pins = "gpio10"; // bsp
		function = "gpio";

		drive-strength = <2>;
		bias-disable;
	};

	sy6288_en_h: sy6288-en-h {
		pins = "gpio2"; // bsp
		function = "gpio";

		drive-strength = <2>;
		bias-disable;
	};

	// Type-C Over-Current Signal
	sy6288_int: sy6288-int {
		pins = "gpio3"; // bsp
		function = "gpio";
		bias-disable;
	};

	pcie1_default_state: pcie1-default-state {
		perst-n-pins {
			pins = "gpio102"; // bsp
			function = "gpio";

			drive-strength = <2>;
			output-low;
			bias-pull-down;
		};

		clkreq-pins {
			pins = "gpio103"; // bsp
			function = "pci_e1";

			drive-strength = <2>;
			bias-pull-up;
		};

		wake-n-pins {
			pins = "gpio11"; // bsp
			function = "gpio";

			drive-strength = <2>;
			bias-pull-down;
		};
	};

	sdc2_default_state: sdc2-default-state {
		clk-pins {
			pins = "sdc2_clk";
			bias-disable;

			/*
			 * It seems that mmc_test reports errors if drive
			 * strength is not 16 on clk, cmd, and data pins.
			 */
			drive-strength = <16>;
		};

		cmd-pins {
			pins = "sdc2_cmd";
			bias-pull-up;
			drive-strength = <10>;
		};

		data-pins {
			pins = "sdc2_data";
			bias-pull-up;
			drive-strength = <10>;
		};
	};

	sdc2_card_det_n: sd-card-det-n-state {
		pins = "gpio126"; // bsp
		function = "gpio";
		bias-pull-up;
	};
};

