	pcie0_default_state: pcie0-default-state {
		perst-n-pins {
			pins = "gpio35"; // bsp
			function = "gpio";

			drive-strength = <2>;
			output-low;
			bias-pull-down;
		};

		clkreq-pins {
			pins = "gpio36"; // bsp
			function = "pci_e0";

			drive-strength = <2>;
			bias-pull-up;
		};

		wake-n-pins {
			pins = "gpio37"; // bsp
			function = "gpio";

			drive-strength = <2>;
			bias-pull-up;
		};
	};

