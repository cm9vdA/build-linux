&tlmm {
	gpio-reserved-ranges = <28 4>, <40 4>;

	lt9611 {
		lt9611_irq_pin: lt9611-irq-pin { // bsp
			pins = "gpio1";
			function = "gpio";
			bias-disable;
		};

		lt9611_rst_pin: lt9611-rst-pin { // bsp
			pins = "gpio2";
			function = "gpio";
			output-high;
		};
	};

	sdc2_default_state: sdc2-default-state { // bsp
		clk-pins {
			pins = "sdc2_clk";
			drive-strength = <16>;
			bias-disable;
		};

		cmd-pins {
			pins = "sdc2_cmd";
			drive-strength = <16>;
			bias-pull-up;
		};

		data-pins {
			pins = "sdc2_data";
			drive-strength = <16>;
			bias-pull-up;
		};
	};

	wlan_en: wlan-en { // bsp
		pins = "gpio20";
		function = "gpio";
		output-high;
	};

	bt_en: bt-en { // bsp
		pins = "gpio21";
		function = "gpio";
		output-high;
	};

	bt_sw_ctrl: bt-sw-ctrl { // bsp
		pins = "gpio124";
		function = "gpio";
		output-high;
	};

	usb2 {
		vcc5v0_host0_en: vcc5v0-host0-en { // bsp
			pins = "gpio117";
			function = "gpio";
			output-high;
		};

		vcc3v3_host0_en: vcc3v3-host0-en { // bsp
			pins = "gpio112";
			function = "gpio";
			output-high;
		};

		vcc1v2_host0_en: vcc1v2-host0-en { // bsp
			pins = "gpio118";
			function = "gpio";
			output-high;
		};
	};

	vcc3v3_pcie0_en: vcc3v3-pcie0-en {
		pins = "gpio62";
		function = "gpio";
		output-high;
	};

	sdc2_card_det_n: sd-card-det-n-state { // bsp
		pins = "gpio77";
		function = "gpio";
		bias-pull-up;
		drive-strength = <2>;
	};

	display {
		vcc2v8_display_panel_avdd_en: vcc2v8-display-panel-avdd-en { // bsp
			pins = "gpio23";
			function = "gpio";
			output-high;
		};
	};

	camera {
		vcc1v1_cam_vdig_en: vcc1v1-cam-vdig-en { // bsp
			pins = "gpio41";
			function = "gpio";
			output-low;
		};

		vcc1v1_cam_front_vdig_en: vcc1v1-cam-front-vdig-en { // bsp
			pins = "gpio42";
			function = "gpio";
			output-low;
		};

		vcc2v8_cam_avdd_en: vcc2v8-cam-avdd-en { // bsp
			pins = "gpio64";
			function = "gpio";
			output-low;
		};

		vcc2v8_cam_front_avdd_en: vcc2v8-cam-front-avdd-en { // bsp
			pins = "gpio70";
			function = "gpio";
			output-low;
		};
		vcc2v8_actuator_regulator_avdd_en: vcc2v8-actuator-regulator-avdd-en { // bsp
			pins = "gpio120";
			function = "gpio";
			output-low;
		};
		vcc2v8_actuator_regulator_front_avdd_en: vcc2v8-actuator-regulator-front-avdd-en { // bsp
			pins = "gpio88";
			function = "gpio";
			output-low;
		};
	};

	type-c {
		aux_en_gpio: aux-en-gpio { // bsp
			pins = "gpio9";
			function = "gpio";
			bias-pull-up;
			drive-strength = <2>;
		};

		aux_sel_gpio: aux-sel-gpio { // bsp
			pins = "gpio8";
			function = "gpio";
			bias-pull-up;
			drive-strength = <2>;
		};

		usbplug_cc_gpio: usbplug-cc-gpio { // bsp
			pins = "gpio65";
			function = "gpio";
			bias-pull-up;
			drive-strength = <2>;
		};
	};
};

