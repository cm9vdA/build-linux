&tlmm {
	gpio-reserved-ranges = <28 4>, <40 4>;

	audio {
		audio_active: audio-active {
			pins = "gpio10"; // bsp
			function = "gpio";
			output-high;
		};
	};

#if ENABLE_HDMI
	lt9611uxc {
		lt9611_irq_pin: lt9611-irq-pin {
			pins = "gpio59"; // verify
			function = "gpio";
			bias-disable;
		};

		lt9611_rst_pin: lt9611-rst-pin {
			pins = "gpio62"; // verify
			function = "gpio";
			output-high;
		};

		lt9611_3v3_en: lt9611-3v3-en {
			pins = "gpio61"; // verify
			function = "gpio";
			output-high;
		};

		lt9611_1v2_en: lt9611-1v2-en {
			pins = "gpio11";
			function = "gpio";
			output-high;
		};
	};
#endif

	sdc2_default_state: sdc2-default-state { // bsp
		clk-pins {
			pins = "sdc2_clk";
			drive-strength = <16>;
			bias-disable;
		};

		cmd-pins {
			pins = "sdc2_cmd";
			drive-strength = <16>;
			bias-pull-up;
		};

		data-pins {
			pins = "sdc2_data";
			drive-strength = <16>;
			bias-pull-up;
		};
	};

	wlan_en_state: wlan-default-state {
		pins = "gpio20"; // bsp
		function = "gpio";
		drive-strength = <16>;
		output-low;
		bias-pull-up;
	};

	bt {
		bt_en_state: bt-default-state {
			pins = "gpio21"; // bsp
			function = "gpio";
			drive-strength = <16>;
			output-low;
			bias-pull-up;
		};

		bt_sw_ctrl: bt-sw-ctrl {
			pins = "gpio124"; // bsp
			function = "gpio";
			output-high;
		};
	};

	usb_host_regulator: usb2 {
		vcc5v0_host0_en: vcc5v0-host0-en {
			pins = "gpio117"; // bsp
			function = "gpio";
			output-high;
		};

		vcc3v3_host0_en: vcc3v3-host0-en {
			pins = "gpio112"; // bsp
			function = "gpio";
			output-high;
		};

		vcc1v2_host0_en: vcc1v2-host0-en {
			pins = "gpio118"; // bsp
			function = "gpio";
			output-high;
		};
	};

	pcie1_regulator:pcie1-regulator {
		pcie1_en_1: pcie1-en-1 {
			pins = "gpio6"; // bsp
			function = "gpio";
			output-high;
		};
		pcie1_en_2: pcie1-en-2 {
			pins = "gpio48"; // bsp
			function = "gpio";
			output-high;
		};
	};

	pcie2_regulator:pcie2-regulator {
		vcc3v3_pcie2_en: vcc3v3-pcie2-en {
			pins = "gpio111"; // verify
			function = "gpio";
			output-high;
		};
		pcie2_en_1: pcie2-en-1 {
			pins = "gpio7"; // verify
			function = "gpio";
			output-high;
		};
		pcie2_en_2: pcie2-en-2 {
			pins = "gpio89"; // verify
			function = "gpio";
			output-high;
		};
	};

	sdc2_card_det_n: sd-card-det-n-state {
		pins = "gpio77"; // bsp
		function = "gpio";
		bias-pull-up;
		drive-strength = <2>;
	};

	display {
		vcc2v8_display_panel_avdd_en: vcc2v8-display-panel-avdd-en {
			pins = "gpio23"; // bsp
			function = "gpio";
			output-high;
		};

		display_panel_panel_active_1: display-panel-panel-active-1 {
			pins = "gpio75"; // bsp
			function = "gpio";
			output-high;
		};

		// display_panel_panel_active_2: display-panel-panel-active-2 {
		// 	pins = "gpio60"; // bsp
		// 	function = "gpio";
		// 	output-high;
		// };
	};

	type-c {
		aux_en_gpio: aux-en-gpio {
			pins = "gpio9"; // bsp
			function = "gpio";
			bias-pull-up;
			drive-strength = <2>;
		};

		aux_sel_gpio: aux-sel-gpio {
			pins = "gpio8"; // bsp
			function = "gpio";
			bias-pull-up;
			drive-strength = <2>;
		};

		usbplug_cc_gpio: usbplug-cc-gpio {
			pins = "gpio65"; // bsp
			function = "gpio";
			bias-pull-up;
			drive-strength = <2>;
		};
	};
};

