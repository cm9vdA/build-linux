#define ENABLE_HDMI 1

/ {
	model = "Aplux AI Box";
	compatible = "aplux,ai-box", "qcom,sm8250";
	qcom,msm-id = <QCOM_ID_SM8250 0x20001>;
	qcom,board-id = <QCOM_BOARD_ID(SBC, 1, 0) 0>;
	chassis-type = "embedded";

	aliases {
		serial0 = &uart12;
		serial1 = &uart6;
		sdhc2 = &sdhc_2;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

#ifdef CONFIG_SYS_PROMPT
	memory@80000000 {
		device_type = "memory";
		reg = <0x00000000 0x80000000  0x00000000 0x3B800000>,   // 952MB
			  <0x00000000 0xC0000000  0x00000001 0xC0000000>;   // 7GB
	};
#endif

#if ENABLE_HDMI
	hdmi-out {
		compatible = "hdmi-connector";
		type = "a";

		port {
			hdmi_con: endpoint {
				remote-endpoint = <&lt9611_out>;
			};
		};
	};

	vcc1v2_lt9611: vcc1v2-lt9611-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc1v2_lt9611";

		vin-supply = <&vdc_3v3>;
		regulator-min-microvolt = <1200000>;
		regulator-max-microvolt = <1200000>;

		gpio = <&tlmm 11 GPIO_ACTIVE_HIGH>; // verify
		enable-active-high;
		regulator-boot-on;
		regulator-always-on;

		pinctrl-names = "default";
		pinctrl-0 = <&lt9611_1v2_en>;
	};

	vcc3v3_lt9611: vcc3v3-lt9611-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_lt9611";

		vin-supply = <&vdc_3v3>;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;

		gpio = <&tlmm 61 GPIO_ACTIVE_HIGH>; // verify
		enable-active-high;
		regulator-boot-on;
		regulator-always-on;

		pinctrl-names = "default";
		pinctrl-0 = <&lt9611_3v3_en>;
	};
#endif

	thermal-zones {
		camera-thermal {
			thermal-sensors = <&pm8150l_adc_tm 0>;

			trips {
				active-config0 {
					temperature = <125000>;
					hysteresis = <1000>;
					type = "passive";
				};
			};
		};

		conn-thermal {
			thermal-sensors = <&pm8150b_adc_tm 0>;

			trips {
				active-config0 {
					temperature = <125000>;
					hysteresis = <1000>;
					type = "passive";
				};
			};
		};

		mmw-pa1-thermal {
